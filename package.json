{
  "name": "backman",
	"displayName": "BackMan",
	"description": "BackMan",
	"version": "1.1.0",
	"publisher": "tz2119",
	"icon": "images/BackMan.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/tz2119/BackMan.git"
	},
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:backman.excecute",
        "*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "backman.excecute",
				"title": "戻る"
		}],
		"menus": {
			"editor/context": [{
				"when": "editorFocus",
				"command": "backman.excecute",
				"group": "navigation"
			}]
		}	
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
