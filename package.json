{
  "name": "backman",
	"displayName": "BackMan",
	"description": "BackMan",
	"version": "1.0.0",
	"publisher": "tz2119",
	"icon": "images/BackMan.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/tz2119/BackMan.git"
	},
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:backman.execute"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "backman.execute",
            "title": "BackMan"

		}],
		"menus": {
			"editor/context": [{
				"when": "editorFocus",
				"command": "backman.execute",
				"group": "navigation"
			}]
		}	
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.61.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^7.32.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.1",
        "typescript": "^4.4.3",
        "@vscode/test-electron": "^1.6.2"
    }

}
